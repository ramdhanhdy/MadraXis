{
  "name": "madraxis",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:ci": "jest --ci --coverage --coverageReporters=json",
    "test:class-service": "jest src/services/__tests__/classService.test.ts",
    "test:security": "jest src/services/__tests__/classService.security.test.ts",
    "test:enrollment": "jest src/services/class/__tests__/enrollment.test.ts",
    "test:access": "jest src/services/class/__tests__/access.test.ts",
    "test:integration": "jest src/services/__tests__/integration.test.ts",
    "test:all": "jest --testPathPattern='src/services/__tests__|src/services/class/__tests__' --coverage --coverageReporters=json,text-summary",
    "test:all:custom": "bun run src/services/__tests__/runTests.ts",
    "test:report": "npm run test:all:custom && cat src/services/__tests__/test-report.md",
    "lint": "expo lint",
    "security:audit-logging": "bun run ./scripts/security/audit-logging.js",
    "security:audit-input": "bun run ./scripts/security/audit-input.js",
    "security:audit-auth": "bun run ./scripts/security/audit-auth.js",
    "security:fix-logging": "bun run ./scripts/security/fix-logging.js",
    "security:audit": "bun run security:audit-logging && bun run security:audit-input && bun run security:audit-auth",
    "migrate:ui": "bun run scripts/migration/update-imports.js --phase=ui-components",
    "migrate:domains": "bun run scripts/migration/update-imports.js --phase=domains",
    "migrate:lib": "bun run scripts/migration/update-imports.js --phase=lib",
    "migrate:validate": "bun run scripts/migration/validate-imports.js",
    "migrate:rollback": "bun run scripts/migration/rollback.js",
    "migrate:dry-run": "bun run scripts/migration/update-imports.js --dry-run",
    "migrate:test-patterns": "bun run scripts/migration/test-patterns.js",
    "migrate:checkpoint": "bun run scripts/migration/checkpoint-validation.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "e2e:test": "detox test --configuration=ios.sim.debug",
    "e2e:build": "detox build --configuration=ios.sim.debug",
    "e2e:test-android": "detox test --configuration=android.emu.debug",
    "e2e:build-android": "detox build --configuration=android.emu.debug"
  },

  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "^8.4.2",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.50.0",
    "axios": "^1.8.4",
    "expo": "53.0.19",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "^0.14.8",
    "expo-constants": "~17.1.7",
    "expo-custom-assets": "^1.4.1",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-linking": "~7.1.7",
    "expo-router": "^5.1.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "micromatch": "^4.0.8",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "^2.27.2",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "^4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "supabase": "^2.31.8",
    "uuid": "^11.1.0",
    "zod": "^4.0.5",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/generator": "^7.28.0",
    "@babel/parser": "^7.28.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-syntax-typescript": "^7.27.1",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/traverse": "^7.28.0",
    "@babel/types": "^7.28.2",
    "@storybook/react-native": "^9.0.14",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.12",
    "@types/micromatch": "^4.0.9",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.3.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.9",
    "react-test-renderer": "19.0.0",
    "typescript": "^5.8.3"
  },
  "private": true
}
